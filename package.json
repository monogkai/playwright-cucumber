{
  "name": "playwright-cucumber-sample-project",
  "version": "1.0.0",
  "description": "Sample project with Playwright and Cucumber",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "format": "prettier --write 'e2e/**/*.{ts,tsx,json}'",
    "all-tests": "cucumber-js --tags '@automated and not @obsolete and not @excluded'",
    "debug": "HEADLESS=false yarn cucumber-js --tags '@debug'",
    "lint": "eslint .",
    "allure": "npx allure serve reports/allure-results/tests"
  },
  "keywords": [],
  "author": "David Pereira Martins",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "11.0.0",
    "@playwright/test": "^1.47.0",
    "allure-commandline": "2.30.0",
    "allure-cucumberjs": "2.9.2",
    "allure-js-commons": "2.9.2",
    "axios": "^1.7.2",
    "axios-case-converter": "1.1.0",
    "date-fns": "3.6.0",
    "dotenv": "^16.4.5",
    "jwt-decode": "4.0.0",
    "playwright": "^1.47.0",
    "qs": "6.13.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@cucumber/pretty-formatter": "1.0.1",
    "@types/cucumber": "^7.0.0",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "@typescript-eslint/parser": "8.4.0",
    "eslint": "8.53.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-cucumber": "2.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-json-files": "4.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonar": "0.12.0",
    "eslint-plugin-sonarjs": "2.0.2",
    "prettier": "3.0.3",
    "prettier-plugin-gherkin": "3.1.0",
    "ts-node": "10.9.1"
  }
}
